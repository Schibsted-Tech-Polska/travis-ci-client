{
  "name": "schibsted-tech-polska/travis-ci-client",
  "description": "Travis CI API client",
  "version": "0.1.1",
  "type": "library",
  "keywords": ["travis", "api"],
  "homepage": "https://github.com/Schibsted-Tech-Polska/travis-ci-client",
  "license": "MIT",
  "authors": [
    {
      "name": "STP Stavanger Aftenblad Team",
      "email": "sa@schibsted.pl"
    }
  ],
  "support": {
    "email": "sa@schibsted.pl"
  },
  "require": {
    "php": "~7.1",
    "guzzle/guzzle": "3.9.*",
    "psr/log": "1.0.*",
    "symfony/console": "~2.7"
  },
  "require-dev": {
    "sebastian/phpcpd": "2.0.*",
    "phploc/phploc": "2.1.*",
    "phpmd/phpmd": "^2.6",
    "squizlabs/php_codesniffer": "^2.9"
  },
  "autoload": {
    "psr-4": {
      "Stp\\TravisClient\\": "src/Stp/TravisClient"
    }
  },
  "suggests": {
    "ext-redis": "If you want to use RedisTokenLoader"
  },
  "scripts": {
    "post-install-cmd": [
      "npm install"
    ],
    "post-update-cmd": [
      "npm install"
    ]
  }
}
